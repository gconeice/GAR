BB_BEGIN:
	IMM %GR0, 0
	IMM %GR1, 64
	IMM %GR2, 0
BB0:
	ADDI %GR3, %GR0, 0
	LOAD %GR3, %GR3
 	ADDI %GR4, %GR1, 64
	LOAD %GR4, %GR4
	CMP %GR5, %GR4, %GR3
	JL %GR5, BB2
BB1:
	EQ %GR5, %GR3, %GR4
	JE %GR5, BBINC
	ADDI %GR0, %GR0, 1
	J BBBOUND
BBINC:
	ADDI %GR0, %GR0, 1
	ADDI %GR2, %GR2, 1
	J BBBOUND
BB2:	
	ADDI %GR1, %GR1, 1
	J BBBOUND
BBBOUND:	
	EQI %GR5, %GR0, 64
	JE %GR5, BB_END
	EQI %GR5, %GR1, 128
	JE %GR5, BB_END
	J BB0
BB_END:
	HALT
